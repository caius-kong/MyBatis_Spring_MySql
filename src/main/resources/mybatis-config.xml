<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--配置全局属性-->
    <settings>
        <!--&lt;!&ndash;使用jdbc的getGeneratekeys获取自增主键值&ndash;&gt;-->
        <!--<setting name="useGeneratedKeys" value="true"/>-->
        <!--&lt;!&ndash;使用列别名替换别名　　默认true&ndash;&gt;-->
        <!--<setting name="useColumnLabel" value="true"/>-->
        <!--&lt;!&ndash;开启驼峰命名转换Table:create_time到 Entity(createTime)&ndash;&gt;-->
        <!--<setting name="mapUnderscoreToCamelCase" value="true"/>-->

        <!-- 打开延迟加载 的开关 -->
        <setting name="lazyLoadingEnabled" value="true"/>
        <!-- 将积极加载改为消极加载即按需要加载 -->
        <setting name="aggressiveLazyLoading" value="false"/>
        <!-- 开启全局二级缓存(局限性太大,建议redis) -->
        <!-- <setting name="cacheEnabled" value="true"/>-->
    </settings>

    <!-- 别名定义(除了在此处定义,还可以在spring-mybatis.xml中定义) -->
    <!--<typeAliases>-->
        <!-- 批量扫描别名 -->
        <!--<package name="com.kyh.msm.entity"/>-->
    <!--</typeAliases>-->
</configuration>